@model IEnumerable<PROJET_FINAL.Commande>
@{
    ViewBag.Title = "Index";
}


<h2>Les criss de commande</h2>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Des osti de commande</h4>
    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Information</th>
            <th>Date</th>
            <th>Adresse</th>
            <th>Code Postal</th>
        </tr>
        @foreach (Commande commande in Model)
        {
            <tr>
                <td>@commande.InfosSup</td>
                <td>@commande.DateComm</td>
                <td>@commande.Adresse</td>
                <td>@commande.CodePostal</td>
            </tr>
        }
    </table>
</body>
</html>

