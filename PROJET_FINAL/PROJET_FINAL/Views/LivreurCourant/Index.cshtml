
@model IEnumerable<PROJET_FINAL.Commande>
@{
    ViewBag.Title = "Index";
}


<h2>Mes commandes à faire</h2>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Id Client(placeholder)</th>
            <th>Information</th>
            <th>Date</th>
            
        </tr>
        @foreach (Commande commande in Model)
        {
            <tr>
                <td>@commande.IdClient</td>
                <td>@commande.InfosSup</td>
                <td>@commande.DateComm</td>
                
            </tr>
        }
    </table>
</body>
</html>
